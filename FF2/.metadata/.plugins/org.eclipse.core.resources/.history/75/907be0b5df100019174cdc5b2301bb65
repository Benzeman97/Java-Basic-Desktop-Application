package shop_management_system;

import java.sql.Date;

public class ItemInfo {

    private int _id;
    private String _name;
    private Date _date;
    
    public ItemInfo(int _id,String _name,Date _date)
    {
    	this._id=_id;
    	this._name=_name;
    	this._date=_date;
    }
    public int Id()
    {
    	return _id;
    }
    public void Id(int id)
    {
    	_id =id;
    }
    public String Name()
    {
    	return _name;
    }
    public void Name(String name)
    {
    	_name=name;
    }
    public Date _Date()
    {
    	return _date;
    }
    public void Date(Date date)
    {
    	_date=date;
    }
}
